{
  "name": "@mistcash/sdk-workspace",
  "version": "0.2.2",
  "private": true,
  "workspaces": [
    "core",
    "config",
    "crypto",
    "react"
  ],
  "scripts": {
    "build": "pnpm embed;pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "clean": "pnpm -r clean",
    "embed": "bash scripts/gnark-embeddings.sh",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md,yml,yaml}\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistcash/sdk.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mistcash/sdk/issues"
  },
  "homepage": "https://github.com/mistcash/sdk#readme"
}